extends layout

-var videos = #{getVideos()}

block content
  h1 Hey
  p Welcome to #{title}

    
  for v in videos
    p #{v}
    video(width='320', height='240', autoplay='' controls='')
      source(src="#{'/videos/'+v}")
      source(src='/video/movie.ogg', type='video/ogg')

      

service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}

